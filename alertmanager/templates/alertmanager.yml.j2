global:
  resolve_timeout: 5m

route:
  group_by: ['alertname']
  group_wait: 30s
  group_interval: 5m
  repeat_interval: 3h
  receiver: 'slack-notifications'

receivers:
  - name: 'slack-notifications'
    slack_configs:
      - api_url: '{{ https://hooks.slack.com/services/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 }}'
        channel: '#alerts'
        send_resolved: true

  
